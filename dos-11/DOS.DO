ECHO ==================
ECHO Install DOS v9-20C
ECHO ==================

ECHO ...set the CPU parameters
SET CPU 11/05
SET CPU 64K

ECHO ...disable undesired devices
SET HK DISABLE
SET RHA DISABLE
SET PTR DISABLE
SET PTP DISABLE
SET DZ DISABLE
SET RL DISABLE
SET RX DISABLE
SET RP DISABLE
SET RQ DISABLE
SET TM DISABLE
SET TQ DISABLE

ECHO ...attach disk
SET RK ENABLE
ATTACH RK0 RK05.DSK

ECHO ...attach tape
SET TC ENABLE
ATTACH TC0 v920c1.dta
SET TC0 LOCKED
ATTACH TC1 v920c2.dta
SET TC1 LOCKED
ATTACH TC2 v920c3.dta
SET TC2 LOCKED

ECHO ...attach line printer
ATTACH LPT lineprinter.txt
