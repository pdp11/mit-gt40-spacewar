SET CPU 11/05
SET CPU 64K

SET HK DISABLE
SET RHA DISABLE
SET DZ DISABLE
SET RL DISABLE
SET RX DISABLE
SET RP DISABLE
SET RQ DISABLE
SET TM DISABLE
SET TQ DISABLE

SET PTR ENABLE
SET PTP ENABLE
SET RK ENABLE
SET TC ENABLE
ATTACH RK0 dos11.dsk

EXPECT "DATE:"
BOOT RK0

SEND "01-JAN-74\r"
EXPECT "TIME:"
CONTINUE

SEND "00:00:00\r"
EXPECT "DIALOGUE?"
CONTINUE

SEND "N\r"
EXPECT "$"
CONTINUE

SEND "LO 1,1\r"
EXPECT "$"
CONTINUE

SEND "R PIP\r"
EXPECT "#"
CONTINUE

ATTACH PTR ../src/space.mac
SEND "DK0:SPACE.MAC<PR:\r"
EXPECT "#"
CONTINUE

ATTACH PTR ../src/gtros.mac
SEND "DK0:GTROS.MAC<PR:\r"
EXPECT "#"
CONTINUE

ATTACH PTR ../src/gtmac.mac
SEND "DK0:GTMAC.MAC<PR:\r"
EXPECT "#"
CONTINUE

ATTACH PTR ../src/ifmac.mac
SEND "DK0:IFMAC.MAC<PR:\r"
EXPECT "#"
CONTINUE

SEND "\003"
EXPECT "."
CONTINUE

SEND "KI\r"
EXPECT "$"
CONTINUE

SEND "R MACRO\r"
EXPECT "#"
CONTINUE

SEND "DK0:,DK0:<GTMAC,IFMAC,SPACE\r"
EXPECT "#"
CONTINUE

SEND "DK0:,DK0:<GTMAC,IFMAC,GTROS\r"
EXPECT "#"
CONTINUE

SEND "\003"
EXPECT "."
CONTINUE

SEND "KI\r"
EXPECT "$"
CONTINUE

SEND "R PIP\r"
EXPECT "#"
CONTINUE

ATTACH PTP space.lda
SEND "PP:<DK0:SPACE.BIN\r"
EXPECT "#"
CONTINUE

ATTACH PTP gtros.lda
SEND "PP:<DK0:GTROS.BIN\r"
EXPECT "#"
CONTINUE

QUIT
